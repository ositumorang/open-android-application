<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.global.android.ContactBlackList"
     >
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        
        <provider android:name="DataProvider"
                  android:authorities="com.global.provider.ContactBlackList">
                </provider>
                
        <activity android:name=".ContactBlackList"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                  android:name=".AddBlackList"
                  android:icon="@drawable/ic_menu_add"
                  android:label="@string/add_black_list"
                  android:theme="@android:style/Theme.Dialog">
        </activity>
        <activity android:name=".EditBlackList"
                  android:label="@string/edit_black_list"
                 android:theme="@android:style/Theme.Dialog">
        </activity>
    </application>
</manifest> 